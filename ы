i = 0
r = 0
a=['вопрос 1',
'вопрос 2',
'вопрос 3',
'вопрос 4',
'вопрос 5']
ggg = ['ответ 1',
'ответ 2',
'ответ 3',
'ответ 4',
'ответ 5']
for i in range (5):
    print(a[i])
    
    
    b = input()
    if b == ggg[i]:
        print("правильный ответ")
        r = r+1
        
    else:
        print("ответ неверный, правильный ответ - ",ggg[i])
        
    i= i+1
print("ваш счет - ",r)
